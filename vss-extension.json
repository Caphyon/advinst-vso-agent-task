{
  "manifestVersion": 1,
  "id": "AdvancedInstaller-BuildTask",
  "version": "1.0.0",
  "name": "Advanced Installer",
  "description": "Tool for building installers.",
  "publisher": "caphyon",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "categories": [
    "Build and release"
  ],
  "links": {
    "home": {
      "uri": "https://www.advancedinstaller.com"
    },
    "learn": {
      "uri": "http://www.advancedinstaller.com/features.html"
    },
    "support": {
      "uri": "http://www.advancedinstaller.com/support.html"
    },
    "repository": {
      "uri": "https://github.com/Caphyon/advinst-vsts-task/"
    },
    "issues": {
      "uri": "https://github.com/Caphyon/advinst-vsts-task/issues/"
    }
  },
  "tags": [
    "build",
    "setup",
    "advanced installer",
    "msi",
    "appv",
    "appx"
  ],
  "icons": {
    "default": "images/advinst-logo.png"
  },
  "screenshots": [
    {
      "path": "images/screen1.png"
    },
    {
      "path": "images/screen2.png"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE.md"
    }
  },
  "galleryFlags": [
    "Public",
    "Preview"
  ],
  "files": [
    {
      "path": "task",
      "addressable": true
    },
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "README.md",
      "addressable": true
    },
    {
      "path": "LICENSE.md",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "Caphyon.AdvancedInstaller.BuildTask",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "task"
      }
    }
  ]
}